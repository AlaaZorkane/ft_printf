%[flags][width][.precision][length]type

flags:
- | KO | 100%
0 | KO | 100%
. | KO | 100%
* | KO | 100%

width:
minimum field width | KO | 100%

types:
c | OK
s | KO | WITH ALL
d | OK
i | OK
u | OK
p | OK
x | OK
X | OK
% | OK

RETURN: KO

